<!-- Modal --> 
<div class="modal fade" id="exampleModal" data-bs-backdrop="static" data-bs-keyboard="false" tabindex="-1" 
    aria-labelledby="exampleModalLabel" aria-hidden="true" > 
    <div id="bg-spinner-modal"> 
        <div id="position-spinner"> 
            <span id="spinner"></span> 
        </div> 
    </div> 
    <div class="modal-dialog modal-lg"> 
        <div class="modal-content"> 
            <div class="modal-header"> 
                <h1 class="modal-title fs-5" id="exampleModalLabel">Personalizar la busqueda</h1> 
            </div> 
            <div class="modal-body"> 
                <form method="GET"> 
                    <!-- Row fecha --> 
                    <div class="row"> 
                        <div class="col-6"> 
                            <label for="date" class="col col-form-label text-center">Desde:</label> 
                            <br>
                            <label style="color: rgba(0, 0, 0, 0.364);">MM/DD/AAAA</label>
                            <div class="col-11 text-center"> 
                              <div class="input-group date" id="datepicker"> 
                                <input type="text" class="form-control" name="since" value="{{date_first_day.date}}"> 
                                <span class="input-group-append"> 
                                  <span class="input-group-text bg-white d-block"> 
                                    <i class="fa fa-calendar"></i> 
                                  </span> 
                                </span> 
                              </div> 
                            </div> 
                            <br> 
                          </div> 
                          <div class="col-6"> 
                            <label for="date" class="col col-form-label text-center">Hasta:</label> 
                            <br>
