                                <div class="col-11"> 
                                    <a class="btn btn-primary ven_btn" id="ven_btn" role="button">Listar Vendedores</a> 
                                    <a class="btn btn-primary item_btn" id="item_btn" role="button">Listar Lineas</a> 
                                    <a class="btn btn-primary client_btn" id="client_btn" role="button">Listar Clientes</a> 
                                </div> 
                            </div> 
                        </div> 
                    </div> 
                    <!-- Botones para ocultar --> 
                    <div class="row"> 
                        <div class="col-11"> 
